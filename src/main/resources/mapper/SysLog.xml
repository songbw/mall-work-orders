<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.fengchao.workorders.mapper.SysLogMapper">
    <resultMap id="baseResultMap" type="com.fengchao.workorders.model.SysLog">
        <result column="id" property="id" jdbcType="BIGINT"/>
        <result column="create_time" property="createTime" jdbcType="TIMESTAMP"/>
        <result column="status" property="status" jdbcType="TINYINT"/>
        <result column="ip" property="ip" jdbcType="VARCHAR"/>
        <result column="user" property="user" jdbcType="VARCHAR"/>
        <result column="method" property="method" jdbcType="VARCHAR"/>
        <result column="param" property="param" jdbcType="VARCHAR"/>
        <result column="result" property="result" jdbcType="VARCHAR"/>
        <result column="duration" property="duration" jdbcType="BIGINT"/>
        <result column="url" property="url" jdbcType="VARCHAR"/>
        <result column="user_agent" property="userAgent" jdbcType="VARCHAR"/>
    </resultMap>
    <!--  -->
    <sql id="sql_column">
        id,
        createTime,
        status,
        ip,
        user,
        method,
        param,
        result,
        duration,
        url,
        userAgent
    </sql>

    <sql id="sql_column_where">
        <if test="id !=null ">
            AND id = #{id}
        </if>
        <if test="createTime !=null ">
            AND create_time = #{createTime}
        </if>
        <if test="status !=null ">
            AND status = #{status}
        </if>
        <if test="ip !=null and ip.length() != 0">
            AND ip = #{ip}
        </if>
        <if test="user !=null and user.length() != 0">
            AND user = #{user}
        </if>
        <if test="method !=null and method.length() != 0">
            AND method = #{method}
        </if>
        <if test="param !=null and param.length() != 0">
            AND param = #{param}
        </if>
        <if test="result !=null and result.length() != 0">
            AND result = #{result}
        </if>
        <if test="duration !=null ">
            AND duration = #{duration}
        </if>
        <if test="url !=null and url.length() != 0">
            AND url = #{url}
        </if>
        <if test="userAgent !=null and userAgent.length() != 0">
            AND user_agent = #{userAgent}
        </if>
    </sql>

    <insert id="insert" parameterType="SysLog" useGeneratedKeys="true" keyProperty="id">
        insert into sys_log
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="createTime !=null">
                create_time,
            </if>
            <if test="status !=null">
                status,
            </if>
            <if test="ip !=null and ip.length() !=0">
                ip,
            </if>
            <if test="user !=null and user.length() !=0">
                user,
            </if>
            <if test="method !=null and method.length() !=0">
                method,
            </if>
            <if test="param !=null and param.length() !=0">
                param,
            </if>
            <if test="result !=null and result.length() !=0">
                result,
            </if>
            <if test="duration !=null">
                duration,
            </if>
            <if test="url !=null and url.length() !=0">
                url,
            </if>
            <if test="userAgent !=null and userAgent.length() !=0">
                user_agent,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="createTime !=null">
                #{createTime},
            </if>
            <if test="status !=null">
                #{status},
            </if>
            <if test="ip !=null and ip.length() !=0">
                #{ip},
            </if>
            <if test="user !=null and user.length() !=0">
                #{user},
            </if>
            <if test="method !=null and method.length() !=0">
                #{method},
            </if>
            <if test="param !=null and param.length() !=0">
                #{param},
            </if>
            <if test="result !=null and result.length() !=0">
                #{result},
            </if>
            <if test="duration !=null">
                #{duration},
            </if>
            <if test="url !=null and url.length() !=0">
                #{url},
            </if>
            <if test="userAgent !=null and userAgent.length() !=0">
                #{userAgent},
            </if>
        </trim>
    </insert>

    <delete id="deleteById">
        delete
        from sys_log
        where id =#{id}
    </delete>

    <delete id="deleteByIdList">
        delete
        from sys_log
        WHERE id IN
          <foreach collection="ids" item="id"
                     index="index" open="(" close=")" separator=",">
                #{id}
          </foreach>
    </delete>

    <select id="select" parameterType="SysLog" resultType="SysLog">
        select * from sys_log where
        <where>
            <include refid="sql_column_where"/>
        </where>
    </select>

    <select id="selectById" parameterType="long" resultType="SysLog">
        SELECT *
        FROM sys_log
        WHERE id = #{id} AND status = 1
    </select>

    <select id="selectAll" resultType="SysLog">
        SELECT *
        FROM sys_log
        WHERE status = 1
    </select>

    <select id="selectCounts" resultType="integer">
        SELECT count(*)
        FROM sys_log
        WHERE status = 1
    </select>
    <select id="selectLog" resultMap="baseResultMap">
        SELECT *
        FROM sys_log
        WHERE
        true
        <if test=" method != null and method.length() != 0">
            AND method LIKE concat('%', #{method},'%')
        </if>
        <if test="param != null and param.length() != 0">
            AND param LIKE concat('%', #{param},'%')
        </if>
        <if test="url != null and url.length() != 0 ">
            AND url LIKE concat('%', #{url},'%')
        </if>
        <if test="user != null and user.length() != 0 ">
            AND user LIKE concat('%', #{user},'%')
        </if>
        <if test="createTimeStart !=null ">
            AND create_time &gt;= #{createTimeStart}
        </if>
        <if test="createTimeEnd !=null ">
            AND create_time &lt;= #{createTimeEnd}
        </if>
        <if test="sort !=''">
            ORDER BY ${sort}
        </if>
        <if test="order != ''">
            ${order}
        </if>

    </select>

</mapper>
