<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fengchao.workorders.mapper.WorkOrderXMapper">


  <select id="selectRefundUserCountByMerchantId" parameterType="java.lang.Long" resultType="java.lang.Integer">
    select count(distinct(receiver_id)) from work_orders.work_order where merchant_id = #{merchantId}
  </select>

</mapper>